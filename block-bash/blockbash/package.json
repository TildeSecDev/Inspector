{
  "name": "express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node scripts/kill-port.js 3000 3002 && (command -v docker >/dev/null 2>&1 && docker compose up -d || echo 'Docker not available, skipping compose') && ALLOW_PORT_REUSE=1 node ./bin/www",
    "start:force": "node scripts/kill-port.js 3000 3002 && ALLOW_PORT_REUSE=0 node ./bin/www",
    "prestart": "node scripts/kill-port.js 3000 3002",
    "start:server": "node ./bin/www",
    "start:inspector": "node ./bin/www",
    "test": "node misc/test/pty.js",
    "test:ui": "ALLOW_PORT_REUSE=1 playwright test -c tests/e2e/playwright.config.js",
  "test:ui:all": "ALLOW_PORT_REUSE=1 playwright test -c tests/e2e/playwright.config.js --reporter=list",
    "test:ui:smoke": "ALLOW_PORT_REUSE=1 playwright test -c tests/e2e/playwright.config.js tests/e2e/editor.spec.js",
    "test:postman": "newman run test/postman/BlockBash_Sandbox.postman_collection.json --timeout-request 15000",
    "test:auth": "SKIP_DOCKER=1 jest --runInBand tests/auth.integration.test.js",
    "test:api": "SKIP_DOCKER=1 jest --runInBand tests/auth.integration.test.js tests/password_reset.integration.test.js tests/admin_guard.integration.test.js tests/progress.integration.test.js",
    "test:integration": "SKIP_DOCKER=1 jest --runInBand --testMatch '<rootDir>/tests/*.integration.test.js'",
    "repo:tidy": "node scripts/repo-tidy.js",
    "test:integration:cov": "SKIP_DOCKER=1 jest --runInBand --coverage --testMatch '<rootDir>/tests/*.integration.test.js'",
  "test:ci": "npm run test:ui:smoke && npm run test:integration",
  "test:refresh": "NO_GLOBAL_SERVER=1 SKIP_DOCKER=1 jest --runInBand tests/refresh.integration.test.js",
  "test:selenium": "SKIP_DOCKER=1 jest --runInBand tests/selenium/*.test.js",
  "prune:refresh": "node backend/security/pruneRefreshTokens.js",
  "test:unit": "SKIP_DOCKER=1 jest --runInBand tests/isBanned.test.js",
  "test:ui:headed": "HEADLESS=0 ALLOW_PORT_REUSE=1 playwright test -c tests/e2e/playwright.config.js"
  },
  "dependencies": {
    "assert": "^2.1.0",
    "bcrypt": "^5.1.1",
  "paseto": "^3.1.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dockerode": "^4.0.2",
    "express": "^4.21.2",
    "express-ws": "^5.0.2",
    "http-errors": "~1.6.3",
    "jade": "^0.29.0",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "mongoose": "^8.15.0",
    "morgan": "~1.9.1",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "node-fetch": "^3.3.2",
    "node-postgres": "^0.6.2",
    "node-pty-prebuilt-multiarch": "^0.10.0",
    "pg": "^8.16.3",
    "shelljs": "^0.10.0",
    "socket.io": "^4.7.5",
    "sqlite3": "^5.1.7",
    "ssh2": "^1.15.0",
    "strip-ansi": "^7.1.0",
    "wireshark": "^1.0.2",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ljharb/tsconfig": "^0.3.2",
    "@playwright/test": "^1.54.2",
    "jest": "^29.7.0",
  "newman": "^6.2.0",
  "selenium-webdriver": "^4.23.0",
    "supertest": "^6.3.4"
  },
  "$schema": "https://json.schemastore.org/package",
  "jest": {
    "testEnvironment": "node",
    "globalSetup": "./tests/globalSetup.js",
    "globalTeardown": "./tests/globalTeardown.js",
    "testTimeout": 30000,
    "collectCoverageFrom": [
      "backend/**/*.js",
      "!backend/**/node_modules/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov"],
    "coverageProvider": "v8"
  }
}
