* {
  margin: 0px;
  padding: 0px;
}

 /*Main CSS - Parent*/
main {
  display: flex;
  flex-direction: row;
}

/*About Popup CSS*/
.about-container.about-hidden {
  display: none;
}

.about-container {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10;
  background-color: #4e5dc18e;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.about-container .about-content {
  background-color: #f2f0f0;
  box-shadow: 5px 5px 10px rgba(0, 0, 0, 0.2);
  border-radius: 6px;
  max-width: 750px;
  display: flex;
  flex-direction: column;
}

.about-content h1 {
  font-family: 'Tomorrow', sans-serif;
  text-transform: uppercase;
  font-weight: 600;
  font-size: 30px;
  color: #000;
  padding: 20px;
}

.about-content .abt-divider {
  content: '';
  display: block;
  width: 100%;
  height: 1px;
  background-color: #d4d4d4;
  margin-top: 5px;
}

.about-content .text-space {
  content: '';
  display: block;
  width: 100%;
  height: 1px;
  background-color: #ffffff00;
  margin: 5px 0px;
}

.about-content .abt-info {
  margin-top: 30px;
  margin-bottom: 20px;
}

.about-content p {
  font-family: 'Tomorrow', sans-serif;
  font-weight: 400;
  font-size: 18px;
  color: #3e3e3e;
  padding: 0px 20px 5px 20px;
  max-width: 90%;
}

.about-content .abt-close {
  background-color: #4e5dc1;
  color: #fff;
  font-family: 'Tomorrow', sans-serif;
  font-weight: 500;
  font-size: 18px;
  padding: 10px 20px;
  margin: 15px 15px 15px 0px;
  border-radius: 3px;
  border: none;
  cursor: pointer;
  align-self: flex-end;
}

/*Loading Screen*/
.loading-screen.hide-loading {
  display: none;
}

.loading-screen {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100vh;
  background-color: #4e5dc18e;
  position: fixed;
  z-index: 9999;
}

.loading-screen img {
  width: 15vw;
  height: auto;
  object-fit: contain;
  object-position: center;
  filter: brightness(0) invert(1);
}

@keyframes bounce-spin-shrink {
  0% {
    transform: translateY(0) rotate(0deg) scale(1);
  }
  25% {
    transform: translateY(-20px) rotate(-10deg) scale(1.1);
  }
  50% {
    transform: translateY(0) rotate(10deg) scale(0.9);
  }
  75% {
    transform: translateY(-10px) rotate(-5deg) scale(1.05);
  }
  100% {
    transform: translateY(0) rotate(0deg) scale(1);
  }
}

.loading-screen img {
  animation: bounce-spin-shrink 2s infinite ease-in-out;
}

/*Screen Size Warning CSS*/
.screen-size-warning {
  display: none;
}

@media only screen and (max-width: 900px), only screen and (max-height: 600px) {
  header, main {
    display: none;
  }
  
  .screen-size-warning {
    width: 100%;
    height: 100vh;
    background-color: #000000;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    position: fixed;
    z-index: 9999;
  }

  .screen-size-warning .msg {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    width: 75vw;
    height: 75vh;
    text-align: center;
  }

  .screen-size-warning img {
    width: 100px;
    height: 100px;
    object-fit: contain;
    object-position: center;
    filter: brightness(0) invert(1);
    margin-bottom: 20px;
  }

  .screen-size-warning h1 {
    color: white;
    font-family: 'Tomorrow', sans-serif;
    font-weight: 500;
    font-size: 50px;
    margin-bottom: 30px;
  }

  .screen-size-warning p {
    color: #00FF00;
    font-family: 'Tomorrow', sans-serif;
    font-weight: 500;
    font-size: 20px;
    max-width: 500px;
  }
}

/*Settings Modal CSS*/
.settings-modal.settings-hidden {
  display: none;
}
.settings-modal {
  position: fixed;
  top: 0; left: 0; width: 100vw; height: 100vh;
  background: rgba(30, 30, 30, 0.45);
  z-index: 10000;
  display: flex;
  align-items: center;
  justify-content: center;
}
.settings-content {
  background: #fff;
  color: #222;
  border-radius: 8px;
  box-shadow: 0 4px 24px #0003;
  padding: 32px 32px 24px 32px;
  min-width: 340px;
  max-width: 95vw;
  display: flex;
  flex-direction: column;
  gap: 18px;
}
.settings-content h2 {
  font-family: 'Tomorrow', sans-serif;
  font-size: 1.5rem;
  margin-bottom: 10px;
}
.settings-section {
  margin-bottom: 12px;
  display: flex;
  flex-direction: column;
  gap: 6px;
}
.settings-content label {
  font-weight: 500;
  margin-bottom: 2px;
}
.settings-content input, .settings-content select {
  padding: 6px 10px;
  border-radius: 4px;
  border: 1px solid #bbb;
  font-size: 1rem;
}
.settings-content button {
  background: #4e5dc1;
  color: #fff;
  border: none;
  border-radius: 4px;
  padding: 7px 16px;
  font-size: 1rem;
  margin-top: 4px;
  cursor: pointer;
  transition: background 0.2s;
}
.settings-content button:hover {
  background: #39448e;
}
.settings-close {
  align-self: flex-end;
  background: #bbb;
  color: #222;
  margin-top: 18px;
}
.settings-close:hover {
  background: #888;
}

