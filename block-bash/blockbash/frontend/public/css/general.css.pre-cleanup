/* CRT Flicker Effect */
@keyframes neonFlicker {
  0% { opacity: 1; }
  5% { opacity: 0.95; }
  10% { opacity: 1; }
  15% { opacity: 0.97; }
  20% { opacity: 1; }
  25% { opacity: 0.96; }
  30%, 100% { opacity: 1; }
}
[data-theme="neon"] .terminal,
[data-theme="neon"] .console {
  animation: neonFlicker 5s infinite ease-in-out;
}

