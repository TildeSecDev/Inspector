[data-theme="light"] .about-content button {
  background: #ffffff !important;
  color: #232946 !important;
  border: 1.5px solid #eee !important;
  box-shadow: none !important;
}

[data-theme="light"] .settings-content button:hover,
[data-theme="light"] .about-content button:hover {
  background: #e6e6e6 !important;
  color: #232946 !important;
  border-color: #232946 !important;
}

[data-theme="light"] .settings-close {
  background: #eee !important;
  color: #232946 !important;
  border: 1.5px solid #eee !important;
}

[data-theme="light"] .block-container {
  background: #ffffff !important;
  color: #232946 !important;
  border-color: #eee !important;
}

[data-theme="light"] .console {
  background: #ffffff !important;
  color: #232946 !important;
  border-top: 2px solid #eee !important;
}

[data-theme="light"] .terminal {
  background: #ffffff !important;
  color: #232946 !important;
  font-family: 'Fira Code', 'Source Code Pro', monospace !important;
  text-shadow: none !important;
}

[data-theme="light"] #peek-terminal-btn,
[data-theme="light"] #sidebar-peek-btn,
[data-theme="light"] #sidebar-toggle-btn {
  background: #ffffff !important;
  color: #232946 !important;
  border: 1.5px solid #eee !important;
  box-shadow: none !important;
}
[data-theme="light"] .run-button {
  background: #000000 !important;
}

[data-theme="light"] .run-button:hover,
[data-theme="light"] .run-button img:hover,
[data-theme="light"] #peek-terminal-btn:hover,
[data-theme="light"] #sidebar-peek-btn:hover,
[data-theme="light"] #sidebar-toggle-btn:hover {
  color: #000000 !important;
}

[data-theme="light"] .screen-size-warning,
[data-theme="light"] .loading-screen {
  background: #ffffff !important;
  color: #232946 !important;
}

[data-theme="light"] .screen-size-warning h1,
[data-theme="light"] .screen-size-warning p {
  color: #232946 !important;
}

[data-theme="classic"] {
  background: #ffffff !important;
  color: #232946 !important;
}

[data-theme="classic"] body,
[data-theme="classic"] main,
[data-theme="classic"] .workspace,
[data-theme="classic"] .canvas,
[data-theme="classic"] .console,
[data-theme="classic"] .info-panel,
[data-theme="classic"] .sidebar,
[data-theme="classic"] .block-container,
[data-theme="classic"] .settings-content,
[data-theme="classic"] .about-content {
  background: #ffffff !important;
  color: #232946 !important;
  border-color: #e0e0e0 !important;
}

[data-theme="classic"] header {
  background: #4B5BD5 !important;
  color: #fff !important;
}
[data-theme="classic"] header .button-container button,
[data-theme="classic"] header .button-container button *,
