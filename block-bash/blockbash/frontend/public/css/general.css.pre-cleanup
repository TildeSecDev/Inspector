  color: #fff !important;
  border-color: #232946 !important;
}
[data-theme="classic"] .settings-content,
[data-theme="classic"] .about-content {
  background: #fff !important;
  color: #232946 !important;
  border: 2px solid #e0e0e0 !important;
  box-shadow: 0 0 16px #e0e0e0;
}
[data-theme="classic"] .settings-content label,
[data-theme="classic"] .settings-content h2,
[data-theme="classic"] .about-content h1 {
  color: #4B5BD5 !important;
  text-shadow: none !important;
}
[data-theme="classic"] .settings-content input,
[data-theme="classic"] .settings-content select,
[data-theme="classic"] .settings-content button,
[data-theme="classic"] .about-content button {
  background: #fff !important;
  color: #232946 !important;
  border: 1.5px solid #e0e0e0 !important;
  box-shadow: none !important;
}
[data-theme="classic"] .settings-content button:hover,
[data-theme="classic"] .about-content button:hover {
  background: #f5f5f5 !important;
  color: #232946 !important;
  border-color: #4B5BD5 !important;
}
[data-theme="classic"] .settings-close {
  background: #e0e0e0 !important;
  color: #232946 !important;
  border: 1.5px solid #e0e0e0 !important;
}
[data-theme="classic"] .screen-size-warning,
[data-theme="classic"] .loading-screen {
  background: #fff !important;
  color: #232946 !important;
}
[data-theme="classic"] .screen-size-warning h1,
[data-theme="classic"] .screen-size-warning p {
  color: #232946 !important;
}
[data-theme="classic"] .objective-panel {
  background: rgba(0,0,0,0.85) !important;
  color: #fff !important;
  border: 2px solid #ffe066 !important;
  box-shadow: 0 0 16px #ffe06644;
}
[data-theme="classic"] .objective-panel .objective-title {
  color: #fff !important;
  font-weight: bold;
}
[data-theme="classic"] .objective-panel .objective-keyword {
  color: #ffe066 !important;
  font-weight: bold;
}
[data-theme="classic"] .objective-panel .objective-command {
  background: #111 !important;
  color: #fff !important;
  border-radius: 4px;
  padding: 2px 8px;
  font-family: 'Fira Code', monospace;
  font-size: 1em;
  margin: 0 2px;
  display: inline-block;
}
[data-theme="classic"] .right-panel {
  background: #101a3a !important;
  color: #fff !important;
  background-image: url('/assets/images/pixel-bg.png');
  background-size: cover;
  background-repeat: repeat;
}
[data-theme="classic"] .laptop-img {
  filter: brightness(0.2) !important;
}

.sidebar #sidebar-toggle-btn {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 0 0px 0;
  width: 100%;
  height: 32px;
  font-size: 2.1em;
  background: #ffe066;
  border: none;
  color: #fff;
  cursor: pointer;
  border-radius: 0;
  box-sizing: border-box;
  transition: background 0.2s;
}